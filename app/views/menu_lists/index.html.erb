<p id="notice"><%= notice %></p>

<h1>Menu Lists</h1>

<table id="menulist">
  <thead>
    <tr>
      <th>Menu Item</th>
      <th>Price</th>
      <th>Review and Ratings</th>
    </tr>
  </thead>

  <tbody>
    <% @menu_list.each do |menu_list| %>
      <tr>
        <td><%= menu_list.menu_item %></td>
        <td><%= menu_list.price %></td>
          <td>
            "<form method="post" action="/reviews_and_ratings/new">
              <div>
                <input value="Rate" type="submit" />
              </div>
            </form>"
          </td>
      </tr>
    <% end %>
  </tbody>
  
  
</table>

